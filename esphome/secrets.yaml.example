# ESPHome Secrets Example
# Copy this file to secrets.yaml and fill in your values
# NEVER commit secrets.yaml to version control

# WiFi credentials
wifi_ssid: "your_wifi_ssid"
wifi_password: "your_wifi_password"

# Fallback AP password (used when WiFi connection fails)
ap_password: "fallback_ap_password"

# OTA update password
ota_password: "your_ota_password"

# API encryption key (32-byte base64-encoded key)
# Generate with: openssl rand -base64 32
api_encryption_key: "REPLACE_WITH_32_BYTE_BASE64_KEY=="
